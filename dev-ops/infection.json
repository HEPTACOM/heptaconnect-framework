{
    "timeout": 30,
    "source": {
        "directories": [
            "src/Core/src",
            "src/DatasetBase/src",
            "src/PortalBase/src",
            "src/StorageBase/src"
        ]
    },
    "mutators": {
        "@default": true,
        "DecrementInteger": {
            "ignore": [
                "Heptacom\\HeptaConnect\\Dataset\\Base\\Date"
            ]
        },
        "IncrementInteger": {
            "ignore": [
                "Heptacom\\HeptaConnect\\Dataset\\Base\\Date"
            ]
        }
    },
    "phpUnit": {
        "configDir": "../test"
    },
    "logs": {
        "text": ".build/infection-log.txt"
    }
}
