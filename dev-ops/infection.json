{
    "timeout": 5,
    "source": {
        "directories": [
            "src/Core",
            "src/DatasetBase",
            "src/PortalBase",
            "src/StorageBase",
            "src/TestSuitePortal",
            "src/TestSuiteStorage",
            "src/UiAdminBase"
        ]
    },
    "mutators": {
        "@default": true,
        "global-ignoreSourceCodeByRegex": [
            "\\$this->logger.*"
        ],
        "DecrementInteger": {
            "ignore": [
                "Heptacom\\HeptaConnect\\Dataset\\Base\\Date"
            ],
            "ignoreSourceCodeByRegex": [
                "1[67][0-9]{8}"
            ]
        },
        "IncrementInteger": {
            "ignore": [
                "Heptacom\\HeptaConnect\\Dataset\\Base\\Date"
            ],
            "ignoreSourceCodeByRegex": [
                "1[67][0-9]{8}"
            ]
        }
    },
    "phpUnit": {
        "configDir": "../test"
    },
    "logs": {
        "text": ".build/infection-log.txt"
    }
}
