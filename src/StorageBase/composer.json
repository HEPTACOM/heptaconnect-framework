{
    "name": "heptacom/heptaconnect-storage-base",
    "description": "HEPTAconnect base storage that every other storage provider is based on",
    "license": [
        "AGPL-3.0-or-later",
        "proprietary"
    ],
    "type": "library",
    "keywords": [
        "heptaconnect-storage"
    ],
    "authors": [
        {
            "name": "HEPTACOM GmbH"
        }
    ],
    "homepage": "https://heptaconnect.io",
    "support": {
        "issues": "https://github.com/HEPTACOM/heptaconnect-framework/issues",
        "source": "https://github.com/HEPTACOM/heptaconnect-framework",
        "docs": "https://heptaconnect.io/",
        "rss": "https://heptaconnect.io/news/rss2.xml"
    },
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "bentools/iterable-functions": ">=1.4 <2",
        "heptacom/heptaconnect-dataset-base": "self.version",
        "heptacom/heptaconnect-portal-base": "self.version"
    },
    "repositories": {
        "heptaconnect-sources": {
            "type": "path",
            "url": "../**"
        }
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Storage\\Base\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "php-http/discovery": true
        }
    }
}
