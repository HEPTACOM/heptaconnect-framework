{
    "name": "heptacom/heptaconnect-repository-base",
    "description": "HEPTAconnect package",
    "license": [
        "AGPL-3.0-or-later",
        "proprietary"
    ],
    "type": "library",
    "keywords": [
        "heptaconnect-dataset",
        "heptaconnect-adapter"
    ],
    "authors": [
        {
            "name": "HEPTACOM GmbH"
        }
    ],
    "support": {
        "issues": "https://github.com/HEPTACOM/heptaconnect-repository-base/issues",
        "source": "https://github.com/HEPTACOM/heptaconnect-repository-base",
        "docs": "https://heptaconnect.io/",
        "rss": "https://heptaconnect.io/news/rss2.xml"
    },
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "infection/infection": ">=0.28",
        "phpunit/phpunit": ">=11 <12"
    },
    "conflict": {
        "nikic/php-parser": "4.7.0"
    },
    "repositories": {
        "heptaconnect-sources": {
            "type": "path",
            "url": "../**"
        }
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "ocramius/package-versions": true
        }
    },
    "homepage": "https://heptaconnect.io"
}
